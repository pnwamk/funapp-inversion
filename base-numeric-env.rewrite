          (-> base Nonnegative-Real Boolean : (-PS* -tt (-is-type 0 non-neg)))
          (-> Nonnegative-Real base Boolean : (-PS* (-is-type 1 pos) -tt))
          (-> base Nonpositive-Real Boolean : (-PS* (-is-type 0 neg) -tt))
